FUNCTION CALCULAEXISTENCIA()
	PARAMETERS XCOMANDA,XPROD,XDETALLE
	SELECT SUM(CANTIDAD)-(SUM(FACTURADO)+SUM(TRANSACCION)) AS SALDO;
	FROM DETALLE_COMANDA WHERE COMANDA = XCOMANDA AND PRODUCTO=XPROD AND DETALLE=XDETALLE;
	INTO CURSOR CURSALDO GROUP BY PRODUCTO,DETALLE 
	SELECT DETALLE_COMANDA
	TABLEUPDATE(.T.)
	RETURN CURSALDO.SALDO 



