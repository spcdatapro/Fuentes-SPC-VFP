CLOSE ALL
USE NOTAS IN 0

SELECT * FROM DETALLE_PRODUCTO WHERE BETWEEN(DETALLE_PRODUCTO,341,352) INTO CURSOR LOSEXTRAS

SELECT LOSEXTRAS

SCAN
	SELECT NOTAS
	APPEND BLANK
	REPLACE DESCRIPCION WITH "EXTRA "+ALLTRIM(LOSEXTRAS.DESCRIPCION)
	REPLACE TIPO WITH 1
	REPLACE SUBGRUPO WITH 9
	REPLACE SOLOSUB WITH 1
	REPLACE POS WITH LOSEXTRAS.DETALLE_PRODUCTO
	REPLACE POSDET WITH 1
	
ENDSCAN 

CLOSE ALL
